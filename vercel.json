{
  "version": 2,
  "builds": [
    {
      "src": "smsapi.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9"
      }
    }
  ],
  "routes": [
    { "src": "/", "dest": "smsapi.py" },
    { "src": "/docs", "dest": "smsapi.py" },
    { "src": "/openapi.json", "dest": "smsapi.py" }
  ]
}
