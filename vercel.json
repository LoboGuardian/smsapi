{
  "version": 2,
  "builds": [
    {
      "src": "api/smsapi.py",
      "use": "@now/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/send_messages",
      "dest": "api/smsapi.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/smsapi.py"
    }
  ]
}
